# compile jmh sources all the way to ready to run bench class files
> jmh:compile

# check if benchmark scala code was compiled
$ exists target/scala-2.10/classes/org/openjdk/jmh/samples/JMHSample_01_HelloWorld.class

# check if benchmark java source was generated
$ exists target/scala-2.10/generated-sources/jmh/org/openjdk/jmh/samples/generated/JMHSample_01_HelloWorld_wellHelloThere.java

# check if generated benchmark was compiled
$ exists target/scala-2.10/classes/org/openjdk/jmh/samples/generated/JMHSample_01_HelloWorld_wellHelloThere$BlackHole_1_jmh.class
